func vec3_dot(a: Vec3, b: Vec3): float {
	return a.x*b.x + a.y*b.y + a.z*b.z;
}

struct Vec3 { x: float; y: float; z: float; }

func fib(n: int): int {
	result := 1;
	for (i := 1; i<n; i++) {
		result += i;
	}
	return result;
}
